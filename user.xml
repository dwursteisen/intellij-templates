<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="unit test">
  <template name="that" value="org.fest.assertions.api.Assertions.assertThat($OBJECT$)" description="that / fest assert" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="OBJECT" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="ext" value="org.fest.assertions.api.Assertions.extractProperty($PROPERTY$).from($ITERABLE$)" description="extracte property" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="PROPERTY" expression="&quot;&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="ITERABLE" expression="variableOfType(Type)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="should" value="@org.junit.Test&#10;public void should_$NAME$() {&#10;&#10;}" description="create a junit test" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="should_NG" value="@org.testng.annotations.Test&#10;public void should_$NAME$() {&#10;&#10;}" description="create a test NG unit test" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>

